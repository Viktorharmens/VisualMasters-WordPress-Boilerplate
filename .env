## =================
## Database settings
## =================
DB_NAME=
DB_USER=
DB_PASSWORD=

## ==================
## Optional variables
## ==================
DB_HOST=localhost
DB_PREFIX=vmst_

## =======================================================
## Environment and base URL
## =======================================================
WP_HOME=http://
WP_SITEURL=${WP_HOME}/wp
WP_DEBUG=false

## =======================================================
## Setup the FTP connection info for Gulp file
## =======================================================
FTP_HOST=
FTP_USER=
FTP_PASS=
FTP_PATH=


## ================================
## Set update behaviour (optional)
## Optional. Set WP_AUTO_UPDATE to:
## true:    All updates
## minor:   Only minor WP updates
## false:   No updates (default)
## ================================
# WP_AUTO_UPDATE_CORE=true
# DISABLE_WP_CRON=false
# AUTOMATIC_UPDATER_DISABLED=false
# WP_UPDATE_PLUGINS=true

## ==========================================================
## Be sure to generate a salt ( https://roots.io/salts.html )
## ==========================================================
AUTH_KEY='generateme'
SECURE_AUTH_KEY='generateme'
LOGGED_IN_KEY='generateme'
NONCE_KEY='generateme'
AUTH_SALT='generateme'
SECURE_AUTH_SALT='generateme'
LOGGED_IN_SALT='generateme'
NONCE_SALT='generateme'

